{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "jsxImportSource": "astro"
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
